FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
	(0 0 0)
	(2 0 0)
	(2 2 0)
	(0 2 0)
	(0 0 2)
	(2 0 2)
	(2 2 2)
	(0 2 2)
);

blocks
(
	hex (0 1 2 3 4 5 6 7) (41 41 41) simpleGrading (1 1 1)
);


boundary
(
	zeroFlux
	{
		type patch;
		faces
		(
			(3 7 6 2)
			(0 4 7 3)
			(2 6 5 1)
			(0 3 2 1)
			(4 5 6 7)
			(1 5 4 0)
		);
	}
);

